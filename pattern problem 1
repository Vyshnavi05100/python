n=int(input())
first,lin=1,n
for i in range(1,n+1):
    col,last=lin-1,first
    for j in range(i):
        print((last:=last-(col+j)*(j>=1)),end=" " if j!=i-1 else '\n')
    first,lin=first+lin,lin-1

