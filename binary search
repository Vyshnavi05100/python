def binary_search(a,key):
    s=0
    e=len(a)-1
    while s<=e:
        mid=(s+e)//2
        if a[mid]==key:
            return mid
        elif a[mid]>key:
            e=mid-1
        else:
            s=mid+1
    return -1

a=[5,8,10,12,41,45,81,108]   #it works in sorted list
print(binary_search(a,81))

output:

6

